{
  "name": "prot-table",
  "version": "0.6.0",
  "author": {
    "name": "Robert Seidler",
    "email": "robert.seidler1@gmail.com",
    "url": "https://protronic-gmbh.de/"
  },
  "license": "MIT",
  "homepage": "https://flaviocopes.com/package",
  "keywords": [
    "web-component",
    "vue",
    "table"
  ],
  "description": "A customizable table component in vue. Intended to be used as a web-component.",
  "repository": "github:protronic/prot-table",
  "main": "dist/vue-component/prot-table.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production --dest dist/web-component --target wc --name prot-table && cp public/web-component.html dist/index.html && cp public/data.json dist/data.json",
    "buildlib": "vue-cli-service build --mode production --dest dist/vue-component --target lib --name prot-table ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "lodash.debounce": "^4.0.8",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
